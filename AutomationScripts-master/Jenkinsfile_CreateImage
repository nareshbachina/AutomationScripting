node {
  
   stage('Install python dependencies') {
       sh '''
         cd AutomationScripts-master/AWS/RANDOM
         pip install -r requirements.txt
       '''
   }
   stage('Run Python Scripts') {
       sh '''
       cd AutomationScripts-master/AWS/RANDOM
       python Create_Image.py --instance_name="$INSTANCE_NAME"
      '''
   }
}

